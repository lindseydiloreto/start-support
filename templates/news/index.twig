{% extends '_layout' %}
{% set title = 'News' %}

{% block content %}
    <h1 class="font-bold text-2xl">News</h1>
    <ul>
        {% for entry in craft.entries.section('news').all() %}
            <li><a href="{{ entry.url }}">{{ entry.title }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
